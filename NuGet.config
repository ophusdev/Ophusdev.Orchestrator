<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="ophusdev" value="https://nuget.pkg.github.com/ophusdev/index.json" protocolVersion="3" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="ophusdev">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
	<packageSourceCredentials>
		<ophusdev>
			<add key="Username" value="%TOOOOMM_NUGET_USERNAME%" />
			<add key="ClearTextPassword" value="%TOOOOMM_NUGET_PASSWORD%" />
		</ophusdev>
	</packageSourceCredentials>
</configuration>
